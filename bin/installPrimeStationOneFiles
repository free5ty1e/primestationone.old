#!/bin/bash
PSOFOLDER="~/primestationone"

sudo chmod +x "$PSOFOLDER/bin/*"
sudo chmod +x "$PSOFOLDER/RetroPie/roms/settings/*.sh"
sudo cp "$PSOFOLDER/bin/*" /usr/bin/
sudo cp -r "$PSOFOLDER/etc" /
sudo cp -r "$PSOFOLDER/opt" /
cp -r "$PSOFOLDER/RetroPie/*" ~/RetroPie/
cp -r "$PSOFOLDER/.emulationstation/*" ~/.emulationstation/
cp "$PSOFOLDER/.bashrc" ~/
